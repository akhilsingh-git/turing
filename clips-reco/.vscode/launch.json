    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_DEBUG": "1",
                "CLIENT_ID": "dummy_client_id",
                "CLIENT_SECRET": "dummy_client_secret",
                "video_score_matrix_model_date": "abc",
                "video_score_matrix_model_type": "abc",
                "video_score_matrix_model_arch": "abc",
                "video_score_matrix_model_archive_name": "abc",
                "video_score_matrix_model_content": "abc",
                "watch_history_graph_model_date": "abc",
                "watch_history_graph_model_type": "abc",
                "watch_history_graph_model_arch": "abc",
                "watch_history_graph_model_archive_name": "abc",
                "watch_history_graph_model_content": "abc",
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload",
                "--port=8000"
            ],
            "jinja": true,
            "justMyCode": true
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}